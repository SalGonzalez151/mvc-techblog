{{#each dashboard }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
    
      <a href="/dashboard/{{id}}">
        {{title}}
      </a>
    </h2>

    <p>Created by {{user.user}} on {{format_date upload_date}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{description}}
    </p>
  </div>
</div>
{{/each}}